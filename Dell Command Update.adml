		<presentationTable>

			<!-- Advanced Driver Restore Presentation -->
			<presentation id="DCU_AdvancedDriverRestore">
				<checkBox refId="chkEnableAdvancedRestore" defaultChecked="true">Enable Advanced Restore</checkBox>
				<checkBox refId="chkUseCustomLibrary" defaultChecked="false">Use the specified driver library</checkBox>
				<textBox refId="txtLibraryLocation"><label /></textBox>
			</presentation>

			<!-- BIOS Presentation -->
			<!-- DCU_BIOS_SuspendBitLocker presentation not required -->

			<!-- General Presentation -->
			<presentation id="DCU_General_DownloadFileLocation"><textBox refId="txtDownloadFileLocation"><label>Download File Location</label></textBox></presentation>
			<presentation id="DCU_General_UpdateSourceLocation">
				<multiTextBox refId="txtUpdateSourceLocation">Update Source Locations</multiTextBox>
				<checkBox refId="chkDefaultSource" defaultChecked="false">Default Source Location (dell.com)</checkBox>
			</presentation>

			<!-- DCU_General_DefaultSourceLocation not required -->

			<!-- Internet Proxy Presentation -->
			<presentation id="DCU_General_InternetProxy">
				<checkBox refId="chkUseCustomProxy" defaultChecked="false">Use custom proxy </checkBox>
				<textBox refId="txtProxyServer"><label>Proxy Server</label></textBox>
				<decimalTextBox refId="txtProxyPort" spin="false">Proxy Port</decimalTextBox>
				<checkBox refId="chkUseProxyAuthentication" defaultChecked="false">Use proxy authentication</checkBox>
				<textBox refId="txtProxyUsername"><label>Username</label></textBox>
				<textBox refId="txtProxyPassword"><label>Password</label></textBox>
			</presentation>

			<!-- DCU_General_UserConsent presentation not required -->

			<!-- Update Filter Presentation -->
			<presentation id="DCU_UpdateFilter_DeviceCategory">
				<checkBox refId="chkAudio" defaultChecked="true">Audio</checkBox>
				<checkBox refId="chkChipset" defaultChecked="true">Chipset</checkBox>
				<checkBox refId="chkInput" defaultChecked="true">Input (e.g. Mouse, Keyboard)</checkBox>
				<checkBox refId="chkNetwork" defaultChecked="true">Network and Bluetooth</checkBox>
				<checkBox refId="chkStorage" defaultChecked="true">Storage (e.g. Hard drive, CD/DVD)</checkBox>
				<checkBox refId="chkVideo" defaultChecked="true">Video</checkBox>
				<checkBox refId="chkDeviceOther" defaultChecked="true">All Others</checkBox>
			</presentation>
			<presentation id="DCU_UpdateFilter_RecommendationLevel">
				<checkBox refId="chkSecurity" defaultChecked="true">Security Updates</checkBox>
				<checkBox refId="chkCritical" defaultChecked="true">Critical Updates</checkBox>
				<checkBox refId="chkRecommended" defaultChecked="true">Recommended Updates</checkBox>
				<checkBox refId="chkOptional" defaultChecked="true">Optional Updates</checkBox>
			</presentation>
			<presentation id="DCU_UpdateFilter_UpdateType">
				<checkBox refId="chkDrivers" defaultChecked="true">Hardware Drivers</checkBox>
				<checkBox refId="chkApplications" defaultChecked="true">Application Software (e.g. Command|Update)</checkBox>
				<checkBox refId="chkBios" defaultChecked="true">BIOS Updates</checkBox>
				<checkBox refId="chkFirmware" defaultChecked="true">Firmware Updates</checkBox>
				<checkBox refId="chkUtility" defaultChecked="true">Utility Software</checkBox>
				<checkBox refId="chkTypeOther" defaultChecked="true">All Others</checkBox>
			</presentation>

			<!-- Update Settings Presentation -->
			<presentation id="DCU_UpdateSettings_Action">
				<dropdownList refId="lstUpdateAction" noSort="true">When updates are found</dropdownList>
				<checkBox refId="chkDisableNotify">Disable Notifications</checkBox>
			</presentation>
			<presentation id="DCU_UpdateSettings_UpdateSchedule">
				<dropdownList refId="lstUpdateScheduleMode" noSort="true">Check for updates</dropdownList>
				<dropdownList refId="lstUpdateScheduleTime">Select the time</dropdownList>
				<dropdownList refId="lstUpdateScheduleDay" noSort="true">Select the day (weekly)</dropdownList>
				<dropdownList refId="lstUpdateScheduleDate">Select the day (monthly)</dropdownList>
			</presentation>

			<!-- DCU_UserInterface_LockSettings presentation not required -->

			<!-- DCU_UserInterface_ShowPopup presentation not required -->

